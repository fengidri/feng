        <!--ul>
            % for file in file_list :
            <li><a href='wiki/chapter/{{ file  }}'>{{file}}</a><br /></li>
            %end
        </ul-->
    </div-->
</div>
<script src="http://lab.julying.com/weather/v3/jquery.weather.build.min.js?parentbox=body&moveArea=all&zIndex=1&move=1&drag=1&autoDrop=0&styleSize=big&style=default&time=1385455795&area=client"></script>
</body>
